{
  extends: ['config:base'],

  enabledManagers: ['circleci', 'npm', 'nvm'],
  npmrc: '@fortawesome:registry=https://npm.fontawesome.com/',
  rangeStrategy: 'bump',
  reviewersFromCodeOwners: true,

  force: {
    constraints: {
      node: '< 17.0.0',
    },
  },
  hostRules: [
    {
      hostType: 'npm',
      matchHost: 'https://npm.fontawesome.com',

      encrypted: {
        token: '',
      },
    },
  ],
  packageRules: [
    {
      matchDatasources: ['docker'],
      enabled: false,
    },
    {
      matchDatasources: ['docker'],
      matchPackagePatterns: ['node'],
      enabled: true,
    },
    {
      matchDatasources: ['orb'],
      enabled: false,
    },

    {
      matchManagers: ['npm'],

      addLabels: ['javascript'],
      internalChecksFilter: 'strict',
      postUpdateOptions: ['yarnDedupeHighest'],
      rangeStrategy: 'bump',
      stabilityDays: 3,
    },
    {
      matchManagers: ['npm'],
      matchUpdateTypes: ['minor', 'patch'],

      automerge: true,
      ignoreTests: true,
      platformAutomerge: true,
    },

    {
      groupName: 'datadog',
      matchManagers: ['npm'],
      matchPackagePrefixes: ['@datadog'],
    },
    {
      groupName: 'relay',
      matchManagers: ['npm'],
      matchPackagePrefixes: [
        '@types/react-relay',
        '@types/relay',
        'babel-plugin-relay',
        'react-relay',
        'relay',
      ],
      matchSourceUrlPrefixes: [
        'https://github.com/DefinitelyTyped/DefinitelyTyped',
        'https://github.com/facebook/relay',
      ],
    },
    {
      groupName: 'stylelint',
      matchManagers: ['npm'],
      matchSourceUrlPrefixes: ['https://github.com/stylelint/'],
    },
  ],
}
